{
  "name": "node-server-nodemon-babel-watch",
  "version": "1.0.0",
  "main": "lib/index.js",
  "watch": {
    "start": {
      "patterns": ["lib"],
      "extensions": "js"
    }
  },
  "scripts": {
    "build": "babel lib -d dist",
    "start": "npm run build && nodemon dist/index.js",
    "watch": "npm-watch"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "mocha": "5.2.0",
    "nodemon": "1.17.5"
  },
  "dependencies": {
    "bcrypt": "2.0.1",
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "jsonwebtoken": "8.4.0",
    "jwt-simple": "0.5.5",
    "lodash": "4.17.11",
    "mongodb": "3.1.13",
    "node-pre-gyp": "0.12.0",
    "npm": "6.7.0",
    "npm-watch": "0.6.0"
  }
}
